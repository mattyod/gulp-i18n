{
  "name": "gulp-i18n",
  "version": "0.0.0",
  "description": "Tools for building internationalised templates",
  "main": "index.js",
  "scripts": {
    "hooks": "chmod +x hooks/*; cd .git/hooks && ln -sf ../../hooks/* .",
    "lint": "./node_modules/jshint/bin/jshint ./",
    "jscs": "./node_modules/jscs/bin/jscs ./",
    "unit": "./node_modules/mocha/bin/mocha test",
    "test": "npm run lint && npm run jscs && npm run unit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mattyod/gulp-i18n.git"
  },
  "keywords": [ "i18n" ],
  "author": "mattyod <mattyod@gmail.com> (http://www.mtod.co.uk/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattyod/gulp-i18n/issues"
  },
  "homepage": "https://github.com/mattyod/gulp-i18n",
  "dependencies": {
    "underscore": "1.6.0",
    "gulp-util": "2.2.20",
    "through2": "0.5.1"
  },
  "devDependencies": {
    "jshint": "2.5.1",
    "jscs": "1.5.8",
    "mocha": "1.20.1",
    "chai": "1.9.1",
    "sinon": "1.10.2",
    "sinon-chai": "2.5.0",
    "gulp": "3.8.2",
    "event-stream": "3.1.5"
  }
}
